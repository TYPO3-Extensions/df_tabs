<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<ROOT>
		<el>
			<mode>
				<TCEforms>
					<label>LLL:EXT:df_tabs/Resources/Private/Language/locallang.xml:flexform.sheets.general.mode</label>
					<onChange>reload</onChange>
					<config>
						<type>select</type>
						<items type="array">
							<numIndex index="0" type="array">
								<numIndex index="0">LLL:EXT:df_tabs/Resources/Private/Language/locallang.xml:flexform.sheets.general.mode.0</numIndex>
								<numIndex index="1">tt_content</numIndex>
							</numIndex>
							<numIndex index="1" type="array">
								<numIndex index="0">LLL:EXT:df_tabs/Resources/Private/Language/locallang.xml:flexform.sheets.general.mode.1</numIndex>
								<numIndex index="1">pages</numIndex>
							</numIndex>
							<numIndex index="2" type="array">
								<numIndex index="0">LLL:EXT:df_tabs/Resources/Private/Language/locallang.xml:flexform.sheets.general.mode.2</numIndex>
								<numIndex index="1">combined</numIndex>
							</numIndex>
						</items>
					</config>
				</TCEforms>
			</mode>

			<data>
				<TCEforms>
					<label>LLL:EXT:df_tabs/Resources/Private/Language/locallang.xml:flexform.sheets.general.data</label>
					<config>
						<type>group</type>
						<internal_type>db</internal_type>
						<allowed>pages,tt_content</allowed>
						<size>5</size>
						<minitems>0</minitems>
						<maxitems>9999</maxitems>
						<show_thumbs>1</show_thumbs>
						<autoSizeMax>10</autoSizeMax>
					</config>
				</TCEforms>
			</data>

			<titles>
				<TCEforms>
					<label>LLL:EXT:df_tabs/Resources/Private/Language/locallang.xml:flexform.sheets.general.titles</label>
					<config>
						<type>text</type>
						<cols>30</cols>
						<rows>5</rows>
					</config>
				</TCEforms>
			</titles>

			<ajax>
				<TCEforms>
					<label>LLL:EXT:df_tabs/Resources/Private/Language/locallang.xml:flexform.sheets.general.ajax</label>
					<config>
						<type>check</type>
						<default>0</default>
					</config>
				</TCEforms>
			</ajax>

			<enableAutoPlay>
				<TCEforms>
					<label>LLL:EXT:df_tabs/Resources/Private/Language/locallang.xml:flexform.sheets.general.enableAutoPlayInterval</label>
					<config>
						<type>check</type>
						<default>0</default>
					</config>
				</TCEforms>
			</enableAutoPlay>

			<autoPlayInterval>
				<TCEforms>
					<label>LLL:EXT:df_tabs/Resources/Private/Language/locallang.xml:flexform.sheets.general.autoPlayInterval</label>
					<config>
						<type>input</type>
						<size>5</size>
						<max>5</max>
						<default>7000</default>
						<eval>trim,int</eval>
					</config>
				</TCEforms>
			</autoPlayInterval>

			<enableMouseOver>
				<TCEforms>
					<label>LLL:EXT:df_tabs/Resources/Private/Language/locallang.xml:flexform.sheets.general.enableMouseOver</label>
					<config>
						<type>check</type>
						<default>0</default>
					</config>
				</TCEforms>
			</enableMouseOver>
		</el>
	</ROOT>
</T3DataStructure>